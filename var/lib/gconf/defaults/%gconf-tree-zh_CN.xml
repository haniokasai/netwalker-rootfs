<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="desktop">
			<dir name="gnome">
				<dir name="accessibility">
					<dir name="mouse">
						<entry name="animate_cursor">
							<local_schema short_desc="动画指针">
								<longdesc>显示光标停留时间。</longdesc>
							</local_schema>
						</entry>
						<entry name="button_layout">
							<local_schema short_desc="按钮风格">
								<longdesc>点击方式窗口中的按钮风格 (“0”=文本，“1”=图标，“2”=两者都有)</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_show_ctw">
							<local_schema short_desc="显示点击方式窗口">
								<longdesc>显示点击方式窗口</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_mode">
							<local_schema short_desc="增强型点击模式">
								<longdesc>增强型点击模式 (“0”=鼠标手势模式，“1”=窗口模式)</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_gesture_secondary">
							<local_schema short_desc="二次点击手势">
								<longdesc>二次点击的方向 (“0”=左，“1”=右，“2”=上，“3”=下)</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_gesture_drag">
							<local_schema short_desc="拖动手势">
								<longdesc>拖动操作的方向 (“0”=左，“1”=右，“2”=上，“3”=下)</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_gesture_double">
							<local_schema short_desc="双击手势">
								<longdesc>执行双击操作的方向 (“0”=左，“1”=右，“2”=上，“3”=下)</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_gesture_single">
							<local_schema short_desc="单击手势">
								<longdesc>执行单击操作的方向 (“0”=左，“1”=右，“2”=上，“3”=下)</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_time">
							<local_schema short_desc="增强型点击时间间隔">
								<longdesc>触发点击的时间间隔(秒)</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_enable">
							<local_schema short_desc="启用增强型点击">
								<longdesc>启用增强型点击</longdesc>
							</local_schema>
						</entry>
						<entry name="delay_time">
							<local_schema short_desc="二次点击时间间隔">
								<longdesc>触发二次点击的时间间隔(秒)</longdesc>
							</local_schema>
						</entry>
						<entry name="delay_enable">
							<local_schema short_desc="启用二次点击">
								<longdesc>启用二次点击模拟</longdesc>
							</local_schema>
						</entry>
						<entry name="threshold">
							<local_schema short_desc="移动阀值">
								<longdesc>确认一个移动操作所需的像素点</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
			</dir>
		</dir>
		<dir name="apps">
			<entry name="disable_user_plugins">
				<local_schema short_desc="是否禁用用户主目录中的插件">
					<longdesc>是否禁用用户主目录中的插件</longdesc>
				</local_schema>
			</entry>
			<dir name="update-manager">
				<entry name="first_run">
					<local_schema short_desc="初次运行欢迎信息">
						<longdesc>如果设定了该值，初次启动将会显示欢迎信息</longdesc>
					</local_schema>
				</entry>
				<entry name="show_versions">
					<local_schema short_desc="在更新列表中显示版本信息">
						<longdesc>如果设置此键值，主更新列表窗口中将显示版本信息（从某版到某版）。</longdesc>
					</local_schema>
				</entry>
				<entry name="autoclose_install_window">
					<local_schema short_desc="安装完毕后自动关闭安装窗口">
						<longdesc>如果选中，安装结束后会自动关闭安装窗口。</longdesc>
					</local_schema>
				</entry>
				<entry name="check_dist_upgrades">
					<local_schema short_desc="*已废弃的* 检查新的发行版本">
						<longdesc>这个按键已经被废弃了，文件 /etc/update-manager/release-upgrades 会自动检查，如果当前发布版本的一个新版本可用并且要求升级（如果可能）。</longdesc>
					</local_schema>
				</entry>
				<entry name="window_size">
					<local_schema short_desc="窗口大小">
						<longdesc>储存更新管理器对话框的大小</longdesc>
					</local_schema>
				</entry>
				<entry name="show_details">
					<local_schema short_desc="显示升级细节">
						<longdesc>储存扩展器的状态，包括变动列表和描述</longdesc>
					</local_schema>
				</entry>
				<entry name="remind_reload">
					<local_schema short_desc="提醒重载频道列表">
						<longdesc>如果自动检查更新被禁用，你需要手动重新载入频道列表。本选项允许隐藏此情况的提醒。</longdesc>
					</local_schema>
				</entry>
			</dir>
			<dir name="totem">
				<entry name="autoload_subtitles">
					<local_schema short_desc="在载入电影时是否自动载入字幕文件">
						<longdesc>在载入电影时是否自动载入字幕文件</longdesc>
					</local_schema>
				</entry>
				<entry name="screenshot_save_path">
					<local_schema short_desc="“抓取屏幕快照” 对话框的默认位置">
						<longdesc>“抓取屏幕快照”对话框的默认位置，默认为图片目录</longdesc>
					</local_schema>
				</entry>
				<entry name="open_path">
					<local_schema short_desc="“打开...”对话框的默认位置">
						<longdesc>“打开...”对话框的默认位置，默认为当前目录</longdesc>
					</local_schema>
				</entry>
				<entry name="subtitle_encoding">
					<local_schema short_desc="字幕编码">
					</local_schema>
				</entry>
				<entry name="subtitle_font">
					<local_schema short_desc="字幕字体">
						<longdesc>字幕渲染所用的 Pango 字体描述</longdesc>
					</local_schema>
				</entry>
				<entry name="buffer-size">
					<local_schema short_desc="缓冲大小">
						<longdesc>数据解码的提前量最大值(以秒计)</longdesc>
					</local_schema>
				</entry>
				<entry name="network-buffer-threshold">
					<local_schema short_desc="网络缓冲区阀值">
						<longdesc>网络流媒体开始显示之前要缓存的数据量(以秒计)</longdesc>
					</local_schema>
				</entry>
				<entry name="volume">
					<local_schema short_desc="音量">
						<longdesc>音量百分比，在 0 到 100 之间</longdesc>
					</local_schema>
				</entry>
				<entry name="visual_quality">
					<local_schema short_desc="视觉效果质量设定">
						<longdesc>音频可视化的质量设定：“0”为小，“1”为正常，“2”为大，“3” 为极大。</longdesc>
					</local_schema>
				</entry>
				<entry name="window_on_top">
					<local_schema short_desc="是否主窗口显示在最前端">
						<longdesc>是否主窗口显示在别的窗口前面。</longdesc>
					</local_schema>
				</entry>
				<entry name="audio_output_type">
					<local_schema short_desc="使用的音频输出类型">
						<longdesc>使用的音频输出类型：“0”为立体声，“1”为 4 声道输出，“2”为 5.0 声道输出，“3”为 5.1 声道输出，“4”为通过 AC3 输出。</longdesc>
					</local_schema>
				</entry>
				<entry name="debug">
					<local_schema short_desc="是否启用回放引擎的调试功能">
					</local_schema>
				</entry>
				<entry name="deinterlace">
					<local_schema short_desc="启用隔行扫描">
					</local_schema>
				</entry>
				<entry name="shuffle">
					<local_schema short_desc="乱序模式">
					</local_schema>
				</entry>
				<entry name="repeat">
					<local_schema short_desc="重复模式">
					</local_schema>
				</entry>
				<entry name="connection_speed">
					<local_schema short_desc="网络连接速度">
						<longdesc>大致的网络连接速度，用于选择网络上的媒介质量：&quot;0&quot; 适合 14.4 Kbps Modem, &quot;1&quot; 适合 19.2 Kbps Modem, &quot;2&quot; 适合 28.8 Kbps Modem, &quot;3&quot; 适合 33.6 Kbps Modem, &quot;4&quot; 适合 34.4 Kbps Modem, &quot;5&quot; 适合56 Kbps Modem/ISDN, &quot;6&quot; 适合 112 Kbps Dual ISDN/DSL, &quot;7&quot; 适合 256 Kbps DSL/Cable, &quot;8&quot; 适合 384 Kbps DSL/Cable, &quot;9&quot; 适合 512 Kbps DSL/Cable, &quot;10&quot; 适合 1.5 Mbps T1/Intranet/LAN, &quot;11&quot; 适合 Intranet/LAN.</longdesc>
					</local_schema>
				</entry>
				<entry name="auto_resize">
					<local_schema short_desc="文件装入后自动更改画布大小">
					</local_schema>
				</entry>
				<entry name="saturation">
					<local_schema short_desc="视频饱和度">
					</local_schema>
				</entry>
				<entry name="hue">
					<local_schema short_desc="视频色调">
					</local_schema>
				</entry>
				<entry name="contrast">
					<local_schema short_desc="视频对比度">
					</local_schema>
				</entry>
				<entry name="brightness">
					<local_schema short_desc="视频亮度">
					</local_schema>
				</entry>
				<entry name="visual">
					<local_schema short_desc="视觉效果插件名称">
					</local_schema>
				</entry>
				<entry name="show_vfx">
					<local_schema short_desc="不显示视频时显示视觉效果">
						<longdesc>播放纯音频文件时显示视觉效果。</longdesc>
					</local_schema>
				</entry>
				<entry name="lock_screensaver_on_audio">
					<local_schema short_desc="只播放音频时允许启动屏保">
						<longdesc>允许在只播放音频时启动屏保。这对依靠显示器供电的喇叭很有用。</longdesc>
					</local_schema>
				</entry>
			</dir>
			<dir name="update-notifier">
				<entry name="show_apport_crashes">
					<local_schema short_desc="自动显示 apport 崩溃报告">
						<longdesc>如果禁用，用户将无法收到将来的 apport 崩溃报告通知。</longdesc>
					</local_schema>
				</entry>
			</dir>
			<dir name="pointer-capture">
				<entry name="size">
					<local_schema short_desc="锁定区域的范围">
					</local_schema>
				</entry>
			</dir>
			<dir name="gnome-app-install">
				<dir name="state">
					<entry name="window_width">
						<local_schema short_desc="主窗口高度">
						</local_schema>
					</entry>
					<entry name="window_height">
						<local_schema short_desc="主窗口高度">
						</local_schema>
					</entry>
					<entry name="window_maximized">
						<local_schema short_desc="主窗口最大化状态">
							<longdesc>如果为真，主窗口将会最大化。</longdesc>
						</local_schema>
					</entry>
				</dir>
			</dir>
		</dir>
	</dir>
</gconf>
