<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="desktop">
			<dir name="gnome">
				<dir name="accessibility">
					<dir name="mouse">
						<entry name="animate_cursor">
							<local_schema short_desc="حرك المؤشر">
								<longdesc>أظهر الوقت المنقضي كغطاء للمؤشر.</longdesc>
							</local_schema>
						</entry>
						<entry name="button_layout">
							<local_schema short_desc="أسلوب الزر">
								<longdesc>اسلوب الزر في نافذة نوع النقر (&quot;0&quot; = نص, &quot;1&quot; = رمز, &quot;2&quot; = كلاهما)</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_show_ctw">
							<local_schema short_desc="اعرض نافذة نوع النّقر">
								<longdesc>اعرض نافذة نوع النّقر</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_mode">
							<local_schema short_desc="نمط النقر الكامن">
								<longdesc>نمط النقرة الكامنة (&quot;0&quot; = نمط الحركة, &quot;1&quot; = نمط النافذة)</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_gesture_secondary">
							<local_schema short_desc="حركة النقر الثانوي">
								<longdesc>اتجاه أداء النقرة الثانوية (&quot;0&quot; = يسار, &quot;1&quot; = يمين, &quot;2&quot; = أعلى, &quot;3&quot; = أسفل)</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_gesture_drag">
							<local_schema short_desc="حركة نقر السحب">
								<longdesc>اتجاه أداء نقرة السحب (&quot;0&quot; = يسار, &quot;1&quot; = يمين, &quot;2&quot; = أعلى, &quot;3&quot; = أسفل)</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_gesture_double">
							<local_schema short_desc="حركة نقر ثنائي">
								<longdesc>اتجاه أداء النقرة المزدوجة (&quot;0&quot; = يسار, &quot;1&quot; = يمين, &quot;2&quot; = أعلى, &quot;3&quot; = أسفل)</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_gesture_single">
							<local_schema short_desc="حركة نقر وحيد">
								<longdesc>اتجاه أداء النقرة المفردة (&quot;0&quot; = يسار, &quot;1&quot; = يمين, &quot;2&quot; = أعلى, &quot;3&quot; = أسفل)</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_time">
							<local_schema short_desc="وقت النقر الكامن">
								<longdesc>الوقت بالثوان قبل إطلاق نقر</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_enable">
							<local_schema short_desc="فعّل النقر الكامن">
								<longdesc>فعّل النقر الكامن</longdesc>
							</local_schema>
						</entry>
						<entry name="delay_time">
							<local_schema short_desc="وقت النقر الثانوي">
								<longdesc>الوقت بالثوان قبل إطلاق نقر ثانوي</longdesc>
							</local_schema>
						</entry>
						<entry name="delay_enable">
							<local_schema short_desc="فعّل النقر الثانوي">
								<longdesc>فعِّل محاكاة النقر الثانوي</longdesc>
							</local_schema>
						</entry>
						<entry name="threshold">
							<local_schema short_desc="عتبة الحركة">
								<longdesc>المسافة بالبكسل حتى يَتِمَّ التعرف على الحركة</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
			</dir>
		</dir>
		<dir name="apps">
			<entry name="disable_user_plugins">
				<local_schema short_desc="ما إن كنت تريد تعطيل الملحقات الموجودة في دليل منزل المستخدم ">
					<longdesc>ما إن كنت تريد تعطيل الملحقات الموجودة في دليل منزل المستخدم </longdesc>
				</local_schema>
			</entry>
			<dir name="update-manager">
				<entry name="show_versions">
					<local_schema short_desc="أظهر النسخة في قائمة التحديث">
						<longdesc>إذا ضُبط هذا المفتاح ستُظهر نافذة قائمة التحديث الرئيسية معلومات النسخة (من النسخة إلى النسخة)</longdesc>
					</local_schema>
				</entry>
				<entry name="autoclose_install_window">
					<local_schema short_desc="أغلق نافذة التثبيت تلقائياً بعد تثبيت ناجح">
						<longdesc>اذا كان هذا المفتاح مفعّل نافذة التثبيت ستغلق تلقائياً بعد تثبيت ناجح.</longdesc>
					</local_schema>
				</entry>
				<entry name="check_dist_upgrades">
					<local_schema short_desc="*تقادمت* تحسس إصدارة حديثة للتوزيعة">
						<longdesc>هذا المفتاح مُغرَق لصالح الملف /etc/update-manager/release-upgrades
يلتمس تلقائيا إصدارات جديدة من التوزيعة و يعرض الترقية (إن أمكن).</longdesc>
					</local_schema>
				</entry>
				<entry name="window_size">
					<local_schema short_desc="مساحة النافذة">
						<longdesc>يحفظ حجم حوار update-manager</longdesc>
					</local_schema>
				</entry>
				<entry name="show_details">
					<local_schema short_desc="أظهر تفاصيل تحديث">
						<longdesc>يحفظ حالة المُمدِّد الذي يضم قائمة التغييرات و وصفها</longdesc>
					</local_schema>
				</entry>
				<entry name="remind_reload">
					<local_schema short_desc="تذكير لإعادة تحميل قائمة القنوات">
						<longdesc>إذا كان الالتماس الأوتوماتي للتحديثات معطلا فينبغي لك إعادة تحميل قائمة القنوات يدويا. يمكّنك هذا الخيار من إخفاء التنويه التي تظهر في هذه الحالة.</longdesc>
					</local_schema>
				</entry>
			</dir>
			<dir name="totem">
				<entry name="autoload_subtitles">
					<local_schema short_desc="ما إن كان يجب تحميل ملف الترجمة النصيّ آليًا عند تحميل فيلم">
						<longdesc>ما إن كان يجب تحميل ملف الترجمة النصيّ آليًا عند تحميل فيلم</longdesc>
					</local_schema>
				</entry>
				<entry name="subtitle_encoding">
					<local_schema short_desc="ترميز الترجمة">
					</local_schema>
				</entry>
				<entry name="subtitle_font">
					<local_schema short_desc="خط الترجمة">
						<longdesc>وصف بانجو لخط الترحمة</longdesc>
					</local_schema>
				</entry>
				<entry name="buffer-size">
					<local_schema short_desc="حجم التخزين المحلي">
						<longdesc>أقصى حجم من البيانات المفككة قبل العرض (بالثانية)</longdesc>
					</local_schema>
				</entry>
				<entry name="network-buffer-threshold">
					<local_schema short_desc="حدّ تخزين الشبكة">
						<longdesc>كمية البيانات التي سيتم تخزينها من دفوق الشبكة قبل عرضها (بالثواني)</longdesc>
					</local_schema>
				</entry>
				<entry name="volume">
					<local_schema short_desc="شدة الصوت">
						<longdesc>شدّة الصّوت بالدرجة المئوية بين 0 و 100</longdesc>
					</local_schema>
				</entry>
				<entry name="window_on_top">
					<local_schema short_desc="ما إن كان يجب على النافذة الرئيسية أن تبقى في الأعلى">
						<longdesc>ما إن كنت تريد أن تكون النافذة الرئيسية فوق جميع النوافذ الأخرى</longdesc>
					</local_schema>
				</entry>
				<entry name="audio_output_type">
					<local_schema short_desc="نوع خرْج الصوت المُستخدَم">
						<longdesc>نوع خرْج الصوت المُستخدَم: &quot;0&quot; لستيريو و &quot;1&quot; لخرْج بأربع قنوات و &quot;2&quot; لخرْج قناة 5.0 و &quot;3&quot; لخرْج قناة 5.1 و أخيرًا &quot;4&quot; لخرْج AC3.</longdesc>
					</local_schema>
				</entry>
				<entry name="debug">
					<local_schema short_desc="ما إن كنت تريد تمكين التنقيح لمحركّ العرض">
					</local_schema>
				</entry>
				<entry name="deinterlace">
					<local_schema short_desc="فعّل فكّ الدّمج">
					</local_schema>
				</entry>
				<entry name="shuffle">
					<local_schema short_desc="نمط الخلط">
					</local_schema>
				</entry>
				<entry name="repeat">
					<local_schema short_desc="نمط الإعادة">
					</local_schema>
				</entry>
				<entry name="auto_resize">
					<local_schema short_desc="حجّم مساحة الرسم آليًا عند تحميل الملف">
					</local_schema>
				</entry>
				<entry name="saturation">
					<local_schema short_desc="تشبّع ألوان الفيديو">
					</local_schema>
				</entry>
				<entry name="hue">
					<local_schema short_desc="تدرّج ألوان الفيديو">
					</local_schema>
				</entry>
				<entry name="contrast">
					<local_schema short_desc="تباين ألوان الفيديو">
					</local_schema>
				</entry>
				<entry name="brightness">
					<local_schema short_desc="درجة سطوع الفيديو">
					</local_schema>
				</entry>
				<entry name="visual">
					<local_schema short_desc="اسم ملحق المؤثّرات المرئية">
					</local_schema>
				</entry>
				<entry name="show_vfx">
					<local_schema short_desc="إعرض مؤثّرات مرئية عند عدم عرض فيديو">
						<longdesc>إعرض مؤثّرات مرئية عند عزف ملف صوتي فقط.</longdesc>
					</local_schema>
				</entry>
				<entry name="lock_screensaver_on_audio">
					<local_schema short_desc="يسمح بتنشيط حافظة الشاشة حتى ولو كان يعزف ملف صوت فقط">
					</local_schema>
				</entry>
			</dir>
			<dir name="pointer-capture">
				<entry name="size">
					<local_schema short_desc="حجم مساحة الإمساك">
					</local_schema>
				</entry>
			</dir>
		</dir>
	</dir>
</gconf>
