<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="desktop">
			<dir name="gnome">
				<dir name="accessibility">
					<dir name="mouse">
						<entry name="button_layout">
							<local_schema short_desc="Stil for knapp">
							</local_schema>
						</entry>
						<entry name="dwell_show_ctw">
							<local_schema short_desc="Vis vindauge for type trykk">
								<longdesc>Vis vindauge for type trykk</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_mode">
							<local_schema short_desc="Modus for dwell-trykk">
							</local_schema>
						</entry>
						<entry name="dwell_gesture_secondary">
							<local_schema short_desc="Musebevegelse for sekundært klikk">
							</local_schema>
						</entry>
						<entry name="dwell_gesture_drag">
							<local_schema short_desc="Musebevegelse for dra og klikk">
							</local_schema>
						</entry>
						<entry name="dwell_gesture_double">
							<local_schema short_desc="Musebevegelse for dobbeltklikk">
							</local_schema>
						</entry>
						<entry name="dwell_gesture_single">
							<local_schema short_desc="Musebevegelse for enkeltklikk">
							</local_schema>
						</entry>
						<entry name="dwell_time">
							<local_schema short_desc="Tid for dwell-trykk">
								<longdesc>Tid i sekund før eit klikk blir utført</longdesc>
							</local_schema>
						</entry>
						<entry name="dwell_enable">
							<local_schema short_desc="Aktiver dwell-trykk">
								<longdesc>Aktiver dwell-trykk</longdesc>
							</local_schema>
						</entry>
						<entry name="delay_time">
							<local_schema short_desc="Tid for sekundært trykk">
								<longdesc>Tid i sekund før eit sekundært klikk blir utført</longdesc>
							</local_schema>
						</entry>
						<entry name="delay_enable">
							<local_schema short_desc="Aktiver sekundært trykk">
								<longdesc>Slå på simulert sekundært klikk</longdesc>
							</local_schema>
						</entry>
						<entry name="threshold">
							<local_schema short_desc="Minste bevegelse">
								<longdesc>Avstand i pikslar før bevegelse gjenkjennes</longdesc>
							</local_schema>
						</entry>
					</dir>
				</dir>
			</dir>
		</dir>
		<dir name="apps">
			<entry name="disable_user_plugins">
				<local_schema short_desc="Om tillegg i brukarens heimemappa skal deaktiverast">
					<longdesc>Om tillegg i brukarens heimemappa skal deaktiverast</longdesc>
				</local_schema>
			</entry>
			<dir name="update-manager">
				<entry name="check_dist_upgrades">
					<local_schema short_desc="Sjå etter nye utgjevingar av distribusjonen">
						<longdesc>Dette feltet er tatt bort til fordel for fila /etc/update-manager/release-upgrades Sjå etter nyare utgjevingar av distribusjonen og tilby om mogeleg oppgradering.</longdesc>
					</local_schema>
				</entry>
				<entry name="window_size">
					<local_schema short_desc="Vindaugestørrelsen">
						<longdesc>Lagrar størrelsen på oppdateringshansamardialogen</longdesc>
					</local_schema>
				</entry>
				<entry name="show_details">
					<local_schema short_desc="Syn detaljar for ei opdatering">
						<longdesc>Lagrar tilstanden til utvidinga, som inneheld ei lista over endringar og ein beskriving</longdesc>
					</local_schema>
				</entry>
				<entry name="remind_reload">
					<local_schema short_desc="Minn meg på å last kanallista på nytt">
						<longdesc>Dersom automatisk søk etter oppdateringar er deaktivert, må du hente lista over pakkekjelder manuelt. Dette valet lar deg skjule påminninga som er vert vist her.</longdesc>
					</local_schema>
				</entry>
			</dir>
			<dir name="totem">
				<entry name="autoload_subtitles">
					<local_schema short_desc="I undertekstfiler skal lastas automatisk når ein film lastas">
						<longdesc>I undertekstfiler skal lastas automatisk når ein film lastas</longdesc>
					</local_schema>
				</entry>
				<entry name="subtitle_encoding">
					<local_schema short_desc="Koding av tekst">
					</local_schema>
				</entry>
				<entry name="subtitle_font">
					<local_schema short_desc="Skrifttype til teksting">
						<longdesc>Pango-skrifttypeskildring for teksting</longdesc>
					</local_schema>
				</entry>
				<entry name="buffer-size">
					<local_schema short_desc="Bufferstorleik">
						<longdesc>Kor mykje data som skal dekodast før det vert vist (i sekund)</longdesc>
					</local_schema>
				</entry>
				<entry name="network-buffer-threshold">
					<local_schema short_desc="Terskel for nettverksbufring">
						<longdesc>Kor mykje data som skal bufrast for nettverksstraumane før straumen vert vist (i sekund)</longdesc>
					</local_schema>
				</entry>
				<entry name="volume">
					<local_schema short_desc="Lydstyrke">
						<longdesc>Lydstyrke, i prosent mellom 0 og 100.</longdesc>
					</local_schema>
				</entry>
				<entry name="window_on_top">
					<local_schema short_desc="Om hovudvindauget alltid skal vera øvst">
						<longdesc>Om hovudvindauget skal liggja oppå andre vindauge</longdesc>
					</local_schema>
				</entry>
				<entry name="audio_output_type">
					<local_schema short_desc="Type lyd-uttype å bruka">
						<longdesc>Lyd ut-type å bruka. «0» for stereo, «1» for 4-kanals lyd, «2» for 5.0-kanals lyd, «3» for 5.1-kanals lyd, «4» for å senda lyd til AC3.</longdesc>
					</local_schema>
				</entry>
				<entry name="debug">
					<local_schema short_desc="Om avspelingsmotoren skal bruka feilsøking">
					</local_schema>
				</entry>
				<entry name="deinterlace">
					<local_schema short_desc="Bruk linjedobling (deinterlacing)">
					</local_schema>
				</entry>
				<entry name="shuffle">
					<local_schema short_desc="Stokking">
					</local_schema>
				</entry>
				<entry name="repeat">
					<local_schema short_desc="Gjentaking">
					</local_schema>
				</entry>
				<entry name="auto_resize">
					<local_schema short_desc="Endra storleiken på lerretet automatisk ved lasting">
					</local_schema>
				</entry>
				<entry name="saturation">
					<local_schema short_desc="Metninga i videoen">
					</local_schema>
				</entry>
				<entry name="hue">
					<local_schema short_desc="Fargetonen i videoen">
					</local_schema>
				</entry>
				<entry name="contrast">
					<local_schema short_desc="Kontrasten i videoen">
					</local_schema>
				</entry>
				<entry name="brightness">
					<local_schema short_desc="Lysstyrken på videoen">
					</local_schema>
				</entry>
				<entry name="visual">
					<local_schema short_desc="Namn på visualiseringstillegga">
					</local_schema>
				</entry>
				<entry name="show_vfx">
					<local_schema short_desc="Vis visuelle effektar når det ikkje er video å visa">
						<longdesc>Vis visuelle effektar når ei lydfil vert spelt.</longdesc>
					</local_schema>
				</entry>
				<entry name="lock_screensaver_on_audio">
					<local_schema short_desc="La skjermspararen aktiverast sjølv når berre lyd blir avspelt">
					</local_schema>
				</entry>
			</dir>
			<dir name="pointer-capture">
				<entry name="size">
					<local_schema short_desc="Storleik på fangstområde">
					</local_schema>
				</entry>
			</dir>
		</dir>
	</dir>
</gconf>
