<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="totem">
				<entry name="subtitle_encoding">
					<local_schema short_desc="उपशीर्षक एनकोडिंग">
					</local_schema>
				</entry>
				<entry name="subtitle_font">
					<local_schema short_desc="उपशीर्षक फोन्ट">
						<longdesc>उपशीर्षक रेंडरिंग क&apos; पैंगो फोन्ट विवरण</longdesc>
					</local_schema>
				</entry>
				<entry name="buffer-size">
					<local_schema short_desc="बफर आकार">
						<longdesc>प्रदर्शन क&apos; आगाँ डिकोड करब क&apos;लेल आंकड़ा क&apos; अघिकतम मात्रा (सकेंडमे)</longdesc>
					</local_schema>
				</entry>
				<entry name="network-buffer-threshold">
					<local_schema short_desc="नेटवर्क बफरिंग थ्रेसहोल्ड">
						<longdesc>धारा क&apos; प्रदर्शन क&apos; आरंभ हए पहिले सँजाल धारा बफर करब क&apos;लेल आंकड़ा क&apos; अघिकतम मात्रा (सकेंडमे)</longdesc>
					</local_schema>
				</entry>
				<entry name="volume">
					<local_schema short_desc="ध्वनि आयतन">
						<longdesc>ध्वनि आयतन, प्रतिशतमे, 0 आओर 100 क&apos; बीचमे</longdesc>
					</local_schema>
				</entry>
				<entry name="window_on_top">
					<local_schema short_desc="की मुख्य विंडोकेँ शीर्ष पर रहनाइ चाही">
						<longdesc>की मुख्य विंडोकेँ कोनो आन क&apos; शीर्ष पर रहनाइ चाही</longdesc>
					</local_schema>
				</entry>
				<entry name="audio_output_type">
					<local_schema short_desc="प्रयोग क&apos;लेल आडिओ आउटपुट क&apos; प्रकार">
						<longdesc>आडिओ आडिओपुट फाइल क&apos; प्रकार: &quot;0&quot; stereo क&apos;लेल , &quot;1&quot; 4-चैनल आउटपुट क&apos; लेल, &quot;2&quot; 5.0 चैनल आउटपुट क&apos;लेल , &quot;3&quot; 5.1 चैनल आउटपुट क&apos;लेल , &quot;4&quot; AC3 Passthrough.</longdesc>
					</local_schema>
				</entry>
				<entry name="debug">
					<local_schema short_desc="की प्लेबैक इंजन क&apos;लेल डिबगकेँ अक्षम कएनाइ अछि">
					</local_schema>
				</entry>
				<entry name="deinterlace">
					<local_schema short_desc="डिइंटरलेसिंग सक्रिय करू ">
					</local_schema>
				</entry>
				<entry name="shuffle">
					<local_schema short_desc="मोड फेटू">
					</local_schema>
				</entry>
				<entry name="repeat">
					<local_schema short_desc="दोहराबू मोड">
					</local_schema>
				</entry>
				<entry name="auto_resize">
					<local_schema short_desc="फाइल लोड पर कैनवासकेँ स्वतः पुनः आकार दिअ&apos;">
					</local_schema>
				</entry>
				<entry name="saturation">
					<local_schema short_desc="वीडियो क&apos; संतृप्ति">
					</local_schema>
				</entry>
				<entry name="hue">
					<local_schema short_desc="वीडियो क&apos; रँग">
					</local_schema>
				</entry>
				<entry name="contrast">
					<local_schema short_desc="वीडियो क&apos; कंट्रास्ट">
					</local_schema>
				</entry>
				<entry name="brightness">
					<local_schema short_desc="वीडियो क&apos; चमकीलापन">
					</local_schema>
				</entry>
				<entry name="visual">
					<local_schema short_desc="दृश्य प्रभाव प्लगिन क&apos; नाम">
					</local_schema>
				</entry>
				<entry name="show_vfx">
					<local_schema short_desc="जखन कोनो वीडियो प्रदर्शित नहि अछि तँ दृश्य प्रभाव देखाउ">
						<longdesc>सिर्फ आडिओ फाइल क&apos; चलाबै क&apos; दौरान दृश्य प्रभाव देखाउ</longdesc>
					</local_schema>
				</entry>
			</dir>
		</dir>
	</dir>
</gconf>
