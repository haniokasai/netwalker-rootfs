<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<dir name="totem">
				<entry name="window_on_top">
					<local_schema short_desc="Samada tetingkap utama patut kekal di atas">
						<longdesc>Samada tetingkap utama patut kekal di atas yang lain</longdesc>
					</local_schema>
				</entry>
				<entry name="audio_output_type">
					<local_schema short_desc="Jenis output audio untuk diguna">
						<longdesc>jenis output audio untuk digunakan: &quot;0&quot; untuk stereo, &quot;1&quot; untuk output 4-saluran, &quot;2&quot; untuk output  5.0 saluran, &quot;3&quot; untuk output  5.1 saluran, &quot;4&quot; untuk Perentasan AC3.</longdesc>
					</local_schema>
				</entry>
				<entry name="debug">
					<local_schema short_desc="Samasa untuk menghidupkan nyahpepijat bagi jentera pemain">
					</local_schema>
				</entry>
				<entry name="deinterlace">
					<local_schema short_desc="Hidupkan deinterlace">
					</local_schema>
				</entry>
				<entry name="shuffle">
					<local_schema short_desc="Mod kocok">
					</local_schema>
				</entry>
				<entry name="repeat">
					<local_schema short_desc="mod ulangan">
					</local_schema>
				</entry>
				<entry name="auto_resize">
					<local_schema short_desc="Ubahsaiz kanvas secara automatik pada pemuatan fail">
					</local_schema>
				</entry>
				<entry name="saturation">
					<local_schema short_desc="Ketepuan bagi video">
