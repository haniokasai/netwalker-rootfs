					</local_schema>
				</entry>
				<entry name="brightness">
					<local_schema short_desc="Kecerahan bagi video">
					</local_schema>
				</entry>
				<entry name="visual">
					<local_schema short_desc="Nama bagi plugin kesan visual">
					</local_schema>
				</entry>
				<entry name="show_vfx">
					<local_schema short_desc="Papar kesan visual bila tiada video dipapar">
						<longdesc>Papar kesan visual bila memainkan hanya fail audio.</longdesc>
					</local_schema>
				</entry>
			</dir>
		</dir>
	</dir>
</gconf>
