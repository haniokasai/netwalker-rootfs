<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<entry name="disable_user_plugins">
				<local_schema short_desc="प्रयोगकर्ताको गृह डाइरेक्टरीमा प्लगइनहरू अक्षम पार्ने या नपार्ने">
					<longdesc>प्रयोगकर्ताको गृह डाइरेक्टरीमा प्लगइनहरू अक्षम पार्ने या नपार्ने</longdesc>
				</local_schema>
			</entry>
			<dir name="totem">
				<entry name="subtitle_encoding">
					<local_schema short_desc="उपशीर्षक सङ्केतन">
					</local_schema>
				</entry>
				<entry name="subtitle_font">
					<local_schema short_desc="उपशीर्षक फन्ट">
						<longdesc>उपशीर्षक रेन्डरिङका लागि प्याङ्गो फन्ट वर्णन</longdesc>
					</local_schema>
				</entry>
				<entry name="buffer-size">
					<local_schema short_desc="बफर साइज">
						<longdesc>(सेकेन्डमा) प्रदर्शनको अगाडि डिकोड गर्न डेटाको अधिक्तम मात्रा</longdesc>
					</local_schema>
				</entry>
				<entry name="network-buffer-threshold">
					<local_schema short_desc="सञ्जाल बफरिङ थ्रेसहोल्ड">
						<longdesc>(सेकेन्डमा) प्रवाह प्रदर्शन गर्न सुरु गर्नु अघि सञ्जाल प्रवाहका लागि बफर गर्न डेटाको मात्रा</longdesc>
					</local_schema>
				</entry>
				<entry name="volume">
					<local_schema short_desc="ध्वनि भोल्युम">
						<longdesc>० र १०० बीचको, प्रतिशतमा, ध्वनि भोल्युम</longdesc>
					</local_schema>
				</entry>
				<entry name="window_on_top">
					<local_schema short_desc="मुख्य सञ्झ्याल माथि बस्ने छ या छैन">
						<longdesc>मुख्य सञ्झ्याल अन्य एउटाको माथिपट्टी रहनेछ या छैन</longdesc>
					</local_schema>
				</entry>
				<entry name="audio_output_type">
					<local_schema short_desc="प्रयोगका लागि अडियो निर्गतको प्रकार">
						<longdesc>प्रयोगका लागि अडियो निर्गतको प्रकार: स्टेरियोका लागि &quot;०&quot;, ४-च्यानल निर्गतका लागि &quot;१&quot;, ५.० च्यानल निर्गतका लागि &quot;२&quot;, ५.१ च्यानल निर्गतका लागि &quot;३&quot;, AC3 पासथ्रोका लागि &quot;4&quot; ।</longdesc>
					</local_schema>
				</entry>
				<entry name="debug">
					<local_schema short_desc="प्लेब्याकका लागि डिबग सक्षम पार्ने या नपार्ने">
					</local_schema>
				</entry>
				<entry name="deinterlace">
					<local_schema short_desc="डेइन्टरलेसिङ सक्षम पार्नुहोस्">
					</local_schema>
				</entry>
				<entry name="shuffle">
					<local_schema short_desc="शफल मोड">
					</local_schema>
				</entry>
				<entry name="repeat">
					<local_schema short_desc="मोड दोहोर्याउनुहोस्">
					</local_schema>
				</entry>
				<entry name="auto_resize">
					<local_schema short_desc="फाइल लोडमा स्वचालित रूपमा क्यानभास रिसाइज गर्नुहोस्">
					</local_schema>
				</entry>
				<entry name="saturation">
					<local_schema short_desc="भिडियोको संतृप्ति">
					</local_schema>
				</entry>
				<entry name="hue">
					<local_schema short_desc="भिडियोको ह्यु">
					</local_schema>
				</entry>
				<entry name="contrast">
					<local_schema short_desc="भिडियोको व्यतिरेक">
					</local_schema>
				</entry>
				<entry name="brightness">
					<local_schema short_desc="भिडियोको चम्किलोपना">
					</local_schema>
				</entry>
				<entry name="visual">
					<local_schema short_desc="दृश्य प्रभाव प्लगइनको नाम">
					</local_schema>
				</entry>
				<entry name="show_vfx">
					<local_schema short_desc="भिडियो प्रदर्शन नहुँदा दृश्यात्मक प्रभावहरू देखाउनुहोस्">
						<longdesc>एउटा अडियो फाइल मात्र प्ले गर्दा दृश्यात्मक प्रभावहरू देखाउनुहोस् ।</longdesc>
					</local_schema>
				</entry>
			</dir>
		</dir>
	</dir>
</gconf>
