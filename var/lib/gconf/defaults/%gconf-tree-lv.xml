<?xml version="1.0"?>
<gconf>
	<dir name="schemas">
		<dir name="apps">
			<entry name="disable_user_plugins">
				<local_schema short_desc="Ja plugins nav atspējots  lietotāja mājas direktorijā">
					<longdesc>Ja plugins nav atspējots  lietotāja mājas direktorijā</longdesc>
				</local_schema>
			</entry>
			<dir name="update-manager">
				<entry name="autoclose_install_window">
					<local_schema short_desc="Pēc sekmīgas instalēšanas, automātiski aizvērt logu">
						<longdesc>Ja šī atslēga ir uzstādīta, pēc veiksmīgas instalēšanas, logs tiks automātiski aizvērts.</longdesc>
					</local_schema>
				</entry>
				<entry name="check_dist_upgrades">
					<local_schema short_desc="*novecojis* Pārbaudīt jaunu distributīva laidienu">
						<longdesc>Šī iespēja ir novecojusi. Ieteicams izmantot /etc/update-manager/release-upgrades, lai automātiski pārbaudīttu, vai ir pieejama jauna pašreizējās distribūcijas versija un, ja iespējams, piedāvāt atjaunināties.</longdesc>
					</local_schema>
				</entry>
				<entry name="window_size">
					<local_schema short_desc="Loga izmērs">
						<longdesc>Saglabā update-manager dialoga izmēru</longdesc>
					</local_schema>
				</entry>
				<entry name="show_details">
					<local_schema short_desc="Rādīt atjaunināšanas informāciju">
						<longdesc>Saglabā izvērsēja, kas satur izmaiņu sarakstu un aprakstu, stāvokli.</longdesc>
					</local_schema>
				</entry>
				<entry name="remind_reload">
					<local_schema short_desc="Atgādināt par kanālu saraksta pārlādēšanu">
						<longdesc>Ja automātiska jauninājumu pārbaude ir izslēgta, jums manuāli jāpārlādē kanālu saraksts. Šādā gadījumā šī opcija ļauj nerādīt atgādinājumu.</longdesc>
					</local_schema>
				</entry>
			</dir>
			<dir name="totem">
				<entry name="subtitle_encoding">
					<local_schema short_desc="Subtitru kodējums">
					</local_schema>
				</entry>
				<entry name="subtitle_font">
					<local_schema short_desc="Subtitru fonts">
						<longdesc>Pango fonta apraksts subtitru renderēšanai</longdesc>
					</local_schema>
				</entry>
				<entry name="buffer-size">
					<local_schema short_desc="Bufera izmērs">
						<longdesc>Maksimālais datu daudzums, ko dekodēt, pirms rādīšanas (sekundēs)</longdesc>
					</local_schema>
				</entry>
				<entry name="network-buffer-threshold">
					<local_schema short_desc="Tīkla buferēšanas slieksnis">
						<longdesc>Datu daudzums tīkla plūsmām, ko buferēt, pirms sākt tās rādīt (sekundēs)</longdesc>
					</local_schema>
				</entry>
				<entry name="volume">
					<local_schema short_desc="Skaņas skaļums">
						<longdesc>Skaņas skaļums procentos (starp 0 un 100)</longdesc>
					</local_schema>
				</entry>
				<entry name="window_on_top">
					<local_schema short_desc="Vai galvenajam logam jābūt virspusē">
						<longdesc>Vai galvenajam logam jābūt virs citiem logiem</longdesc>
					</local_schema>
				</entry>
				<entry name="audio_output_type">
					<local_schema short_desc="Audio izvada tips, ko lietos">
						<longdesc>Audio izvada tips ko lietos: &quot;0&quot; - stereo, &quot;1&quot; - 4 kanālu izvadam, &quot;2&quot; - 5.0 kanālu izvadam, &quot;3&quot; - 5.1 kanālu izvadam, &quot;4&quot; - AC3 Passthrough.</longdesc>
					</local_schema>
				</entry>
				<entry name="debug">
					<local_schema short_desc="Vai ieslēgt atskaņošanas dzinēja lāgošanu">
					</local_schema>
				</entry>
				<entry name="deinterlace">
					<local_schema short_desc="Ieslēgt atpīšanu (deinterlace)">
					</local_schema>
				</entry>
				<entry name="shuffle">
					<local_schema short_desc="Jaukšanas režīms">
					</local_schema>
				</entry>
				<entry name="repeat">
					<local_schema short_desc="Atkārtošanās režīms">
					</local_schema>
				</entry>
				<entry name="auto_resize">
					<local_schema short_desc="Mainīt audekla izmēru automātiski pie faila ielādes">
					</local_schema>
				</entry>
				<entry name="saturation">
					<local_schema short_desc="Video piesātinājums">
					</local_schema>
				</entry>
				<entry name="hue">
					<local_schema short_desc="Video nokrāsa">
					</local_schema>
				</entry>
				<entry name="contrast">
					<local_schema short_desc="Video kontrasts">
					</local_schema>
				</entry>
				<entry name="brightness">
					<local_schema short_desc="Video spilgtums">
					</local_schema>
				</entry>
				<entry name="visual">
					<local_schema short_desc="Vizuālā efekta spraudņa nosaukums">
					</local_schema>
				</entry>
				<entry name="show_vfx">
					<local_schema short_desc="Parādīt vizuālos efektus, kad video netiek rādīts">
						<longdesc>Parādīt vizuālos efektus, kad tiek atskaņots tikai audio fails.</longdesc>
					</local_schema>
				</entry>
			</dir>
			<dir name="gnome-app-install">
				<entry name="search-timeout">
					<local_schema short_desc="Interaktīvās meklēšanas noildze">
						<longdesc>Noildze starp meklējamā pieprasījuma ievadi un meklēšanas uzsākšanu (milisekundēs)</longdesc>
					</local_schema>
				</entry>
				<entry name="mime-whitelist-components">
					<local_schema short_desc="Piedāvāt lietotni priekš MIME-tipa: apstiprinātie komponenti">
					</local_schema>
				</entry>
				<entry name="components_seen">
					<local_schema short_desc="Komponenti, no kurām ir atļauta lietotņu instalēšana">
					</local_schema>
				</entry>
				<entry name="filter_applications">
					<local_schema short_desc="Rādīt tikai lietotņu apakškopu">
						<longdesc>Iespējamās vērtības: 0 : rādīt visas pieejamās lietotnes 1 : rādīt tikai brīvās lietotnes 2 : - 3 : rādīt tikai atbalstītās lietotnes 4 : rādīt tikai trešo pušu lietotnes 5 : - 6 : rādīt tikai instalētās lietotnes</longdesc>
					</local_schema>
				</entry>
				<dir name="state">
					<entry name="window_width">
						<local_schema short_desc="Galvenā loga platums">
						</local_schema>
					</entry>
					<entry name="window_height">
						<local_schema short_desc="Galvenā loga augstums">
						</local_schema>
					</entry>
					<entry name="window_maximized">
						<local_schema short_desc="Galvenā loga maksimizēšanas stāvoklis">
							<longdesc>Ja patiess, galvenais logs tiks maksimizēts</longdesc>
						</local_schema>
					</entry>
				</dir>
			</dir>
		</dir>
	</dir>
</gconf>
