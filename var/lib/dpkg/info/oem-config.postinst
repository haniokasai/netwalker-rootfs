#! /bin/sh
set -e

. /usr/share/debconf/confmodule

# Automatically added by dh_pycentral
rm -f /var/lib/pycentral/oem-config.pkgremove
if which pycentral >/dev/null 2>&1; then
	pycentral pkginstall oem-config
	if grep -qs '^oem-config$' /var/lib/pycentral/delayed-pkgs; then
		sed -i '/^oem-config$/d' /var/lib/pycentral/delayed-pkgs
	fi
fi
# End automatically added section


exit 0
