#!/bin/sh
set -e
# Automatically added by dh_installmodules
if [ "$1" = install ] || [ "$1" = upgrade ]; then
	if [ -e "/etc/modprobe.d/spider-rootfs-version" ]; then
		if [ "`md5sum \"/etc/modprobe.d/spider-rootfs-version\" | sed -e \"s/ .*//\"`" = \
		     "`dpkg-query -W -f='${Conffiles}' spider-rootfs-version | sed -n -e \"\\\\' /etc/modprobe.d/spider-rootfs-version's/.* //p\"`" ]
		then
			rm -f "/etc/modprobe.d/spider-rootfs-version"
		fi
	fi
fi
# End automatically added section
