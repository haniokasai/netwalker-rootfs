#!/bin/sh

set -e

if [ "$1" = install ] || [ "$1" = upgrade ]; then
	if [ -e "/etc/udev/rules.d/85-brltty.rules" ]; then
		if [ "`md5sum \"/etc/udev/rules.d/85-brltty.rules\" | sed -e \"s/ .*//\"`" = \
		     "`dpkg-query -W -f='${Conffiles}' brltty | sed -n -e \"\\\\' /etc/udev/rules.d/85-brltty.rules's/.* //p\"`" ]
		then
			rm -f "/etc/udev/rules.d/85-brltty.rules"
		fi
	fi
fi




