#!/bin/sh
set -e
# Automatically added by dh_pycentral
rm -f /var/lib/pycentral/python-cairo.pkgremove
if which pycentral >/dev/null 2>&1; then
	pycentral pkginstall python-cairo
	if grep -qs '^python-cairo$' /var/lib/pycentral/delayed-pkgs; then
		sed -i '/^python-cairo$/d' /var/lib/pycentral/delayed-pkgs
	fi
fi
# End automatically added section
