		COMPREPLY=( $( compgen -W '-n -i -v' -- $cur ) )
	fi

}
complete -F _urlsnarf urlsnarf

# webmitm(8) completion
#
_webmitm()
{
	local cur

	COMPREPLY=()
	cur=`_get_cword`

	if [[ "$cur" == -* ]]; then
		COMPREPLY=( $( compgen -W '-d' -- $cur ) )
	else
		_known_hosts
	fi

}
