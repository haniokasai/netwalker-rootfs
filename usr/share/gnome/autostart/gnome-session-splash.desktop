[Desktop Entry]
Type=Application
Name=GNOME Splash Screen
Exec=/usr/lib/gnome-session/helpers/gnome-session-splash
OnlyShowIn=GNOME;
NoDisplay=true
AutostartCondition=GNOME /apps/gnome-session/options/show_splash_screen
